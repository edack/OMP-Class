    "parsest": "node ./scripts/lib/parsest",
    "allocate:data": "c:/Users/edack/git/\"OMP Class\"/build/allocdata.bat",
    "allocate:input": "zowe files cre ps \"Z80843.NCOV19.GLBFILE\" --rl 130 --bs 27950 --zosmf-p OpenCOBOL",
