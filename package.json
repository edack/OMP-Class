    "run:jobts": "zowe jobs submit lf \"./build/runjob.jcl\" --directory \"./output\" --zosmf-p OpenCOBOL",
    "compile:cobolts": "zowe jobs submit lf \"./build/compiler.jcl\" --directory \"./output\" --zosmf-p OpenCOBOL"
  },
